{
  "name": "OnePageApp",
  "version": "1.0.0",
  "description": "Simple start app with Scss, TypeScript, Gulp, Browser-Sync and Jasmine.",
  "directories": {
    "test": "tests"
  },
  "main": "index.js",
  "scripts": {
    "test": "browser-sync start --server --index ./tests/index.html --files='./node_modules, ./devp/assets/js/app.js, ./tests/test.spec.js'",
    "start-devp": "browser-sync start --server devp --files='./devp/assets, ./devp/*.html'",
    "start": "gulp get-production && node index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wanoo21/OnePageApp"
  },
  "bugs": "https://github.com/wanoo21/OnePageApp/issues",
  "homepage": "http://wanoo21.github.io/onepageapp/",
  "keywords": [
    "bootstrap",
    "bourbon",
    "browser-sync",
    "gulp",
    "typescript",
    "start",
    "app"
  ],
  "author": "Ion Prodan <ionprodan33@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "bourbon": "^4.2.6",
    "bourbon-neat": "^1.7.4"
  },
  "devDependencies": {
    "browser-sync": "^2.11.2",
    "gulp-concat": "^2.6.0",
    "gulp-htmlmin": "^1.3.0",
    "gulp-sass": "^2.2.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-typescript": "^2.12.1",
    "jasmine-core": "2.4.1",
    "send": "^0.13.2"
  }
}
